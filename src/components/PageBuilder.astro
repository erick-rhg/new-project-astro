---
import Section from "@components/Section.astro";
const { dataJson } = Astro.props;
const components = {
  Section: Section,
};
---

{
  dataJson.map((data) => {
    const Component = components[data.component];

    if (!Component) {
      return null;
    }

    return (
      <section
        id={data.settings?.saltar_contenido}
        class:list={[data.settings?.layout_class]}
      >
        {" "}
        {/* grid-content */}
        <Component {...data.props} settings={data.settings} />
      </section>
    );
  })
}
